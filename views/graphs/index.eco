<table class="graphs">
  <thead>
    <tr>
      <th class="id">ID</th>
      <th class="name">Name</th>
    </tr>
  </thead>
  <tbody>
    <% for graph in @graphs: %>
      <tr>
        <td class="id">
          <a href="/nodes/<%= @node.id %>/<%= graph.id %>"><%= graph.id %></a>
        </td>
        <td class="name"><%= graph.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
